{
  "author": "Dead Pixels Collective",
  "bugs": {
    "url": "https://github.com/deathbeds/jyve/issues"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.16.2",
    "@phosphor/widgets": "^1.5.0",
    "mock-socket": "^7.1.0",
    "tslib": "^1.9.0"
  },
  "description": "Jupyter Kernels in the Browser",
  "devDependencies": {
    "@types/socket.io": "^1.4.31"
  },
  "files": [
    "{lib,style}/**/*.{css,d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
  "homepage": "https://github.com/deathbeds/jyve",
  "jyve": {
    "help_links": [
      {
        "text": "MDN Web Docs",
        "url": "https://developer.mozilla.org"
      },
      {
        "text": "Can I use ... ?",
        "url": "https://caniuse.com"
      }
    ],
    "language_info": {
      "codemirror_mode": {
        "name": "javascript"
      },
      "file_extension": ".js",
      "mimetype": "text/javascript",
      "name": "javascript",
      "nbconvert_exporter": "javascript",
      "pygments_lexer": "javascript",
      "version": "ES2015"
    }
  },
  "keywords": ["jupyter", "jupyterlab", "jyve"],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "name": "@deathbeds/jyve",
  "repository": {
    "type": "git",
    "url": "https://github.com/deathbeds/jyve.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lab:link": "jupyter labextension link . --no-build",
    "publish:local": "npm publish --registry http://localhost:4873",
    "watch": "tsc --watch"
  },
  "types": "lib/index.d.ts",
  "version": "0.4.1"
}
